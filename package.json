{
  "type": "module",
  "version": "0.7.18",
  "private": true,
  "packageManager": "pnpm@9.12.2",
  "scripts": {
    "release": "bumpp package.json packages/*/package.json --all --execute \"tsx build/bump-php-packages.ts\" --commit \"release: v\"",
    "build": "pnpm -r build",
    "test": "vitest --run",
    "test:all": "pnpm run test && composer test",
    "lint": "eslint \"**/*.{ts,vue,mjs}\"",
    "lint:fix": "eslint \"**/*.{ts,vue,mjs}\" --fix",
    "docs:dev": "vitepress dev docs",
    "docs:build": "vitepress build docs",
    "docs:serve": "vitepress serve docs",
    "ci": "pnpm run lint && pnpm run build && pnpm run test"
  },
  "devDependencies": {
    "@fontsource-variable/rubik": "^5.1.0",
    "@hybridly/core": "workspace:*",
    "@hybridly/utils": "workspace:*",
    "@hybridly/vite": "workspace:*",
    "@hybridly/vue": "workspace:*",
    "@innocenzi/eslint-config": "^0.22.4",
    "@types/debug": "^4.1.12",
    "@types/node": "^20.17.0",
    "@types/qs": "^6.9.16",
    "@types/throttle-debounce": "^5.0.2",
    "@vitest/ui": "^2.1.3",
    "axios": "^1.7.7",
    "bumpp": "^9.7.1",
    "eslint": "^9.13.0",
    "happy-dom": "^14.12.3",
    "hybridly": "workspace:*",
    "msw": "^2.5.1",
    "tailwindcss": "^3.4.14",
    "tsx": "^4.19.1",
    "type-fest": "^4.26.1",
    "typescript": "^5.6.3",
    "unbuild": "^2.0.0",
    "unocss": "^0.63.6",
    "vite": "^5.4.10",
    "vitepress": "^1.4.1",
    "vitest": "^2.1.3",
    "vue": "^3.5.12"
  },
  "pnpm": {
    "overrides": {
      "@vitest/ui": "0.34.6",
      "vitest": "0.34.6"
    },
    "peerDependencyRules": {
      "ignoreMissing": [
        "@algolia/client-search",
        "react",
        "react-dom",
        "@types/react"
      ]
    }
  }
}
