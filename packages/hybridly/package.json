{
  "name": "hybridly",
  "type": "module",
  "version": "0.7.16",
  "description": "Solution to develop server-driven, client-rendered applications",
  "author": "Enzo Innocenzi <enzo@innocenzi.dev>",
  "license": "MIT",
  "funding": "https://github.com/sponsors/innocenzi",
  "homepage": "https://github.com/hybridly/hybridly#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hybridly/hybridly.git"
  },
  "bugs": {
    "url": "https://github.com/hybridly/hybridly/issues"
  },
  "keywords": [
    "hybridly",
    "inertiajs"
  ],
  "publishConfig": {
    "provenance": true,
    "access": "public"
  },
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./vite": {
      "types": "./dist/vite.d.ts",
      "import": "./dist/vite.mjs",
      "require": "./dist/vite.cjs"
    },
    "./vue": {
      "types": "./dist/vue.d.ts",
      "import": "./dist/vue.mjs",
      "require": "./dist/vue.cjs"
    },
    "./client": {
      "types": "./client.d.ts"
    }
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "index.d.ts",
  "files": [
    "*.d.ts",
    "dist"
  ],
  "scripts": {
    "build": "unbuild",
    "stub": "unbuild --stub"
  },
  "dependencies": {
    "@hybridly/core": "workspace:*",
    "@hybridly/vite": "workspace:*",
    "@hybridly/vue": "workspace:*"
  }
}
